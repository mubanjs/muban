{
  "name": "@muban/muban",
  "version": "1.0.0-alpha.9",
  "description": "Writing components for server-rendered HTML",
  "keywords": [
    "muban",
    "component"
  ],
  "repository": "git@github.com:mubanjs/muban.git",
  "author": "Arjan van Wijk <thanarie@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "./dist/esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "lib/**/*",
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "npm-run-all -s clean build:* clean:build",
    "build:ts": "tsc -p ./tsconfig.build.json && shx cp -Rf decl/* . && shx cp -Rf tmp/* .",
    "build:ts-esm": "tsc -p ./tsconfig.build.esm.json",
    "clean": "npm-run-all clean:*",
    "clean:test": "shx rm -rf coverage .nyc_output",
    "clean:build": "shx rm -rf tmp decl",
    "clean:npm": "shx rm -rf lib dist index.js index.d.ts",
    "examples": "webpack serve -c ./examples/webpack.config.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx examples/src",
    "lint:single": "eslint /Volumes/Projects/mm/muban/muban-component/examples/src/components/toggle-expand/ToggleExpand.ts",
    "test": "jest ./src/",
    "cypress": "cypress open",
    "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist/storybook"
  },
  "nodemonConfig": {
    "ignore": [
      "templates.ts"
    ],
    "watch": [
      "./server"
    ]
  },
  "dependencies": {
    "@muban/storybook": "^7.0.0-alpha.6",
    "@types/classnames": "^2.2.11",
    "@types/json-parse-better-errors": "^1.0.0",
    "@types/lodash-es": "^4.17.3",
    "@types/webpack": "^4.41.25",
    "@types/webpack-env": "^1.15.2",
    "@vue/reactivity": "^3.0.2",
    "@vue/runtime-core": "^3.0.2",
    "bootstrap-icons": "^1.1.0",
    "classnames": "^2.2.6",
    "eventemitter3": "^4.0.4",
    "htm": "^3.0.4",
    "html-extract-data": "^1.0.1",
    "isntnt": "^1.4.4",
    "json-parse-better-errors": "^1.0.2",
    "lodash-es": "^4.17.15",
    "ts-dedent": "^1.1.1",
    "vhtml": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-essentials": "^6.1.6",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.2.1",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "cypress": "^4.11.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": ">= 18",
    "eslint-config-airbnb-base": ">= 13",
    "eslint-config-prettier": ">= 4",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-lit": "^1.2.4",
    "eslint-plugin-lit-a11y": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-unicorn": "^21.0.0",
    "file-loader": "^6.2.0",
    "gsap": "./examples/src/vendor/gsap-bonus.tgz",
    "jest": "^26.3.0",
    "lazysizes": "^5.2.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.1.0",
    "postcss-nesting": "^7.0.1",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^2.0.5",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "shx": "^0.3.3",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "svgo-loader": "^2.2.1",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "vuepress": "^1.7.1",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
